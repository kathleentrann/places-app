{
	"expo": {
		"name": "Great Places",
		"description": "Remember all the memorable places you visited by simply taking a picture and selecting the location! Available on android and ios mobile devices.",
		"slug": "great-places",
		"privacy": "public",
		"platforms": ["ios", "android"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/places.png",
		"splash": {
			"image": "./assets/splash_icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#171717"
		},
		"updates": {
			"fallbackToCacheTimeout": 3000
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/places-adaptive.png",
				"backgroundColor": "#171717"
			}
		}
	}
}
